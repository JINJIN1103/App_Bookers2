<!--ログイン後のほぼ共通パーツ　ここから-->
 <h2>User info</h2>
  <%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %>
  <table class="table">
    <tbody>
      <tr>
        
      </tr>
      <tr>
        <th>name</th>
        <th><%= user.name %></th>
      </tr>
       <tr>
        <th>introduction</th>
        <th><%= user.introduction %></th>
      </tr>
  </tbody>
  </table>
  
   
    <dev class=row><%= link_to "プロフィール編集", edit_user_path(user) %></dev>
  
  
<h2 class="mt-3">New book</h2>
<%= form_with model: book, local:true do |f| %>
  <div class="form-group">
    <label>Title </label>
    <%= f.text_field :title %>
  </div>
  <div class="form-group">
    <label>Opinion </label>
    <%= f.text_area :body %>
  </div>
  <div class="form-group">
  <%= f.submit 'Create Book' %>
  </div>
<% end %>