<h1>booksのindexページ</h1>

<% @books.each do |book| %>
<div>
  <p>ショップ名：<%= book.title %></p>
  <p>ユーザーネーム：<%= book.body %></p>
</div>
<% end %>

<%= form_with model: @book, local:true do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>内容</h4>
  <%= f.text_area :body %>
  <%= f.submit '投稿' %>
<% end %>